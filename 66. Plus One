class Solution(object):
    def plusOne(self, digits):
        l=[]
        s=0
        k=len(digits)
        for i in range(0,k):
            s=s*10+digits[i]
        s=s+1
        while(s>0):
            b=s%10
            l.append(b)
            s=s//10
        return l[::-1]
            
            
        
